{
  "name": "template-vite-react",
  "private": true,
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "description": "Boilerplate for React + Vite project",
  "homepage": "https://github.com/aquia-inc/template-vite-react#readme",
  "bugs": {
    "url": "https://github.com/aquia-inc/template-vite-react/issues"
  },
  "author": "Aquia, Inc.",
  "contributors": [
    "Sinan Bolel"
  ],
  "type": "module",
  "packageManager": "yarn@4.5.0",
  "engines": {
    "node": "20",
    "npm": "please-use-yarn",
    "yarn": "4.5.0"
  },
  "scripts": {
    "start": "yarn run dev",
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build --mode production",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "test": "NODE_ENV=test jest --coverage --colors --maxWorkers=50%",
    "test:precommit": "NODE_ENV=test jest --onlyChanged --coverage --colors --watch=false --maxWorkers=50%",
    "test:watch": "NODE_ENV=test jest --onlyChanged --coverage --colors --watch --maxWorkers=25%",
    "test:ci": "NODE_ENV=test jest --ci --all  --coverage --colors --maxWorkers=100%",
    "ci": "run-p 'lint:js' 'lint:other' 'test:ci'",
    "lint": "run-p 'lint:js' 'lint:other'",
    "lint:js": "prettier --check '**/*.{ts,tsx,js,jsx,cjs}' --ignore-path .gitignore --ignore-path .prettierignore",
    "lint:other": "prettier --check '**/*.{json,md,yaml,yml}' --ignore-path .gitignore --ignore-path .prettierignore",
    "fix": "run-p 'fix:**'",
    "fix:js": "yarn run lint:js --write",
    "fix:other": "yarn run lint:other --write",
    "semantic-release": "semantic-release",
    "postinstall": "yarn run prepare",
    "prepare": "husky install",
    "sb": "yarn run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "^5.0.0-alpha.176",
    "@mui/material": "^5.17.1",
    "@mui/x-data-grid": "^6.20.4",
    "@popperjs/core": "^2.11.8",
    "aws-amplify": "^5.3.27",
    "classnames": "^2.5.1",
    "clipboard-copy": "^4.0.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-animate-height": "^3.2.3",
    "react-cookie": "^6.1.3",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.54.2",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.30.1",
    "uswds": "^2.14.0",
    "uuid": "^9.0.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.20",
    "@storybook/client-api": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@storybook/react-vite": "^7.6.20",
    "@storybook/testing-library": "^0.2.2",
    "@swc/core": "^1.11.21",
    "@swc/jest": "^0.2.38",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.18",
    "@types/node": "^20.19.1",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-helmet": "^6.1.11",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "camelcase": "^6.3.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.10",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.2.2",
    "prop-types": "^15.8.1",
    "react-docgen-typescript": "^2.4.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.89.2",
    "semantic-release": "^24.2.5",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.6.20",
    "typescript": "^5.8.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.4.19",
    "vite-plugin-environment": "^1.1.3",
    "web-vitals": "^3.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.html": "prettier --parser html --fix",
    "**/*.{js,jsx,ts,tsx,cjs}": [
      "yarn fix:js"
    ],
    "**/*.{json,md,yaml,yml}": [
      "yarn fix:other"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "private": true,
    "branches": [
      "main"
    ],
    "tagFormat": "v${version}",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist",
              "label": "Distribution"
            },
            {
              "path": "CHANGELOG.md",
              "label": "Changelog"
            },
            {
              "path": "README.md",
              "label": "Readme"
            }
          ]
        }
      ]
    ],
    "preset": "conventionalcommits"
  }
}
